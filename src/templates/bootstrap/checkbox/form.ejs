<div class="{{ ctx.instance.checkboxClasses }}">
    <{{ctx.input.type}} ref="input"
    {% for (var attr in ctx.input.attr) { %}
    {{attr}}="{{ctx.input.attr[attr]}}"
    {% } %}
    {% if (ctx.checked) { %}checked=true{% } %}
    ></{{ctx.input.type}}>
    <div class="{{ ctx.instance.stateClasses }}">
        <label class="control-label form-check-label"></label>
    </div>
</div>
